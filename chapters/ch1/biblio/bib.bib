@article{valenzanoDisclosingComplexStructure2011,
  title = {Disclosing the {{Complex Structure}} of {{UiO}}-66 {{Metal Organic Framework}}: {{A Synergic Combination}} of {{Experiment}} and {{Theory}}},
  volume = {23},
  issn = {0897-4756},
  doi = {10.1021/cm1022882},
  abstract = {Through a combined use of experimental and theoretical approaches such as XRPD, EXAFS, IR, and UV?vis spectroscopies and ab initio periodic DFT calculations, we report a detailed characterization of structural, vibrational, and electronic properties of UiO-66 (Zr-BDC MOF) in its hydroxylated and dehydroxylated forms. The stability of the materials with respect to the most common solvents, acids, and bases is determined by combining XRPD and TGA/MS techniques. The structures of the two forms of UiO-66 are refined through an interactive XRPD/EXAFS approach and validated by ab initio calculations. Experimental and calculated IR spectra are reported and compared to enlighten the nature of vibrational modes upon dehydroxylation and to show the complete reversibility of the dehydration/hydration phenomenon. Experimental and calculated band gaps are also reported and compared. In this work, we show the necessity to combine, in a synergic way, different experimental techniques and periodic ab initio approaches to disclose and fully understand the nature of complex novel materials such as UiO-66 on structural, vibrational, and electronic grounds. The correct structure refinement could not be possible using one of these three approaches alone, in particular, XRPD data were unable to detect an important distortion of the Zr6O6 units of the dehydrated material that was, however, foreseen in the ab initio calculations and measured in the EXAFS spectra. Through a combined use of experimental and theoretical approaches such as XRPD, EXAFS, IR, and UV?vis spectroscopies and ab initio periodic DFT calculations, we report a detailed characterization of structural, vibrational, and electronic properties of UiO-66 (Zr-BDC MOF) in its hydroxylated and dehydroxylated forms. The stability of the materials with respect to the most common solvents, acids, and bases is determined by combining XRPD and TGA/MS techniques. The structures of the two forms of UiO-66 are refined through an interactive XRPD/EXAFS approach and validated by ab initio calculations. Experimental and calculated IR spectra are reported and compared to enlighten the nature of vibrational modes upon dehydroxylation and to show the complete reversibility of the dehydration/hydration phenomenon. Experimental and calculated band gaps are also reported and compared. In this work, we show the necessity to combine, in a synergic way, different experimental techniques and periodic ab initio approaches to disclose and fully understand the nature of complex novel materials such as UiO-66 on structural, vibrational, and electronic grounds. The correct structure refinement could not be possible using one of these three approaches alone, in particular, XRPD data were unable to detect an important distortion of the Zr6O6 units of the dehydrated material that was, however, foreseen in the ab initio calculations and measured in the EXAFS spectra.},
  number = {7},
  journal = {Chemistry of Materials},
  author = {Valenzano, Loredana and Civalleri, Bartolomeo and Chavan, Sachin and Bordiga, Silvia and Nilsen, Merete H. and Jakobsen, S\o{}ren and Lillerud, Karl Petter and Lamberti, Carlo},
  month = apr,
  year = {2011},
  keywords = {ab initiomodeling,EXAFS,long range order,microporousmaterials,MOF,short range order,structure refinement,XRPD},
  pages = {1700--1718},
